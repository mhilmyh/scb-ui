{
  "name": "frontend-keuanganscb",
  "version": "1.0.0",
  "description": "Frontend using Nuxt",
  "author": "ditdittdittt",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "alt": "nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "prettier": "prettier --write ./**/*.{js,vue}"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.8.5",
    "@nuxtjs/axios": "^5.9.5",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "chance": "^1.1.5",
    "nodemon": "^2.0.4",
    "nuxt": "^2.15.8",
    "nuxt-i18n": "^6.8.1",
    "vue-signature-pad": "^2.0.0"
  },
  "devDependencies": {
    "@mdi/font": "^5.0.45",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "lint-staged": "^10.0.0",
    "prettier": "^1.19.1",
    "stylelint": "^10.1.0"
  }
}
